<div class="container mt-5">
    <h1>Users</h1>
    <table class="table table-hover">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Title</td>
            <td>Email</td>
            <td>Show</td>
            <td>Edit</td>
            <td>Delete</td>

        </tr>
        <%_.each(users, function (user) { %>
        <tr>
            <td><%- user.id %></td>
            <td><%- user.name %></td>
            <td><%- user.title %></td>
            <td><%- user.email %></td>
            <td><a href="/user/show/<%- user.id%>" class="btn btn-success">Show</a></td>
            <td><a href="/user/edit/<%- user.id%>" class="btn btn-warning">Edit</a></td>
            <td><a href="/user/delete/<%- user.id%>" class="btn btn-danger">Delete</a></td>
        </tr>
        <% }) %>
    </table>
</div>
